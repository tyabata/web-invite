{
  "name": "app-invites",
  "version": "1.0.0",
  "description": "招待用",
  "author": "tyabata",
  "private": true,
  "scripts": {
    "dev": "npm run dev-server & nuxt-ts dev",
    "dev-server": "API_HOST=${npm_package_config_bucket} node check.js",
    "generate": "API_HOST=${npm_package_config_bucket} nuxt-ts generate",
    "lint": "tslint --project .",
    "precommit": "npm run lint",
    "setup": "echo 'input your s3-bucket' && read BUCKET && npm config set app-invites:bucket $BUCKET",
    "upload": "aws s3 sync ./dist s3://${npm_package_config_bucket} --include \"*\" --acl public-read --cache-control \"max-age=900\" --profile=cli",
    "deploy-aws": "npm run generate && npm run upload",
    "check-bucket": "echo ${npm_package_config_bucket}",
    "output-dependencies": "npm ls --depth=0 --prod=true --json=true > ./src/assets/json/dependencies.json",
    "test": "jest"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^2.6.0",
    "nuxt-ts": "^2.4.3",
    "vue-class-component": "^6.3.2",
    "vue-property-decorator": "^7.3.0",
    "vuetify": "^1.3.14",
    "vuetify-loader": "^1.0.8"
  },
  "devDependencies": {
    "@nuxtjs/vuetify": "^0.5.4",
    "@types/node": "^10.12.24",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "csswring": "^7.0.0",
    "express": "^4.16.4",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "postcss-import": "^12.0.1",
    "postcss-mixins": "^6.2.1",
    "postcss-nested": "^4.1.1",
    "postcss-preset-env": "^6.5.0",
    "prettier": "1.14.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.15.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "vue-jest": "^3.0.2",
    "vue-tslint-loader": "^3.5.6"
  },
  "engines": {
    "node": "10"
  }
}
